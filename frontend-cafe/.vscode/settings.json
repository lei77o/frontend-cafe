{
  "git.autoRepositoryDetection": "subFolders",
  "git.openRepositoryInParentFolders": "always",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "node_modules": true,
    "dist": true,
    ".angular": true,
    ".git": true,
    ".idea": true
  },
  "cSpell.words": [
    "aggregateid",
    "approvedfrom",
    "approvedto",
    "argparse",
    "Carshield",
    "chargeback",
    "CHARGEOFF",
    "closedfrom",
    "closedto",
    "counterparties",
    "Counterparty",
    "Creds",
    "diffie",
    "dont",
    "ecommerce",
    "Fmts",
    "fundingfrom",
    "fundingto",
    "heldback",
    "imagedefinitions",
    "ISRV",
    "luxon",
    "Mastertemplate",
    "mepco",
    "Micr",
    "Multimap",
    "multimaps",
    "nestjs",
    "nextval",
    "ngneat",
    "nslb",
    "payables",
    "Payhold",
    "payholds",
    "phonepay",
    "pino",
    "PTMS",
    "releasedfrom",
    "releasedto",
    "reportservice",
    "Repost",
    "Roboto",
    "sftphost",
    "sftppassword",
    "sftppath",
    "sftpport",
    "sftpuser",
    "submittedfrom",
    "submittedto",
    "Templatestate",
    "teppeis",
    "timestamptz",
    "Txns",
    "typeorm",
    "UNDISBURSED",
    "unmarshall",
    "Verifi",
    "voidedfrom",
    "voidedto",
    "Worldpay"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "always"
  },
  "typescript.format.semicolons": "insert"
}
